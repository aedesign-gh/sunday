---
---
[
    {% for post in site.posts %}
    {% if post.image %}
    {
      "title"    : "{{ post.title }}",
      "href"     : "{{ post.url }}",
      "img"      : "{{ post.url | remove: '/' }}",
      "dated"     : "{{ post.dated }}"
    }
    {% unless forloop.last %},{% endunless %}
    {% else %}
    {
      "title"    : "{{ post.title }}",
      "href"     : "{{ post.url }}",
      "img"      : "no-prev",   
      "dated"     : "{{ post.dated }}"
    }
    {% unless forloop.last %},{% endunless %}
    {% endif %}
    {% endfor %}
]